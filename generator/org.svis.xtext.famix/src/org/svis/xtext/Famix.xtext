enum MethodType:
	Unknown | Setter | Getter | Constructor;	

Boolean:
	'true' | 'false';
	
IntegerReference:
	'(' 'ref:' ref=[FAMIXElement|INT_ID] ')';
	
FutureReference:
	'(' 'ref:' ref= INT_ID ')';

INT_ID returns ecore::EString:
	'^'? INT;

terminal MSESTRING:
	("'"->"'")*;
