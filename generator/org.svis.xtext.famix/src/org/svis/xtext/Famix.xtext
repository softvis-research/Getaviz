	'(FAMIX.FileAnchor'
	'(' 'id:' name=INT_ID ')'
	('(' 'element' element=IntegerReference ')')?
	'(' 'endLine' endline=INT ')'
	'(' 'fileName' filename=MSESTRING ')'
